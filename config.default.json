{
    "name": "#botName#",
    "prefix": "#botPrefix#",
    "token": "#botToken#",
    "adminIds": [
        "#userId#"
    ],
    "website": "https://github.com/FelixRilling/di-ngy",
    "files": {
        "data": {
            "dir": "./data/",
            "log": "bot",
            "storage": [
                "data"
            ]
        },
        "assets": "./data/assets/"
    },
    "options": {
        "enableDefaultCommands": true,
        "commandsAreCaseSensitive": true,
        "answerToMissingCommand": false,
        "answerToMissingArgs": false,
        "answerToMissingPerms": false
    }
}
