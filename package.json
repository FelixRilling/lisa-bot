{
    "name": "lisa-bot",
    "version": "6.0.0",
    "description": "A discord bot with miscellaneous features",
    "main": "index.js",
    "scripts": {
        "start": "node ./index.js",
        "test:manual": "node test.js",
        "lint": "npm-run-all --parallel lint:es",
        "lint:es": "eslint index.js ./lib/**/*.js",
        "fix": "npm-run-all fix:es fix:prettier",
