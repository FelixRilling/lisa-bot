        "fix:prettier": "prettier index.js ./lib/**/* --write",
        "data:pokemon": "node lib/commands/pokemon/lib/convertData.js"
    },
    "keywords": [
        "discord",
        "bot"
    ],
    "homepage": "https://github.com/FelixRilling/lisa-bot",
    "author": {
        "name": "Felix Rilling",
        "email": "contact@f-rilling.com",
