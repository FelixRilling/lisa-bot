{
    "name": "lisa-bot",
    "version": "8.0.1",
    "private": true,
    "description": "A discord bot with miscellaneous features",
    "main": "./dist/src/main.js",
    "types": "./dist/src/main.d.ts",
    "scripts": {
        "start": "npm-run-all build serve",
        "serve": "node -r source-map-support/register ./dist/src/main.js",
        "dist": "npm-run-all lint build test",
        "build": "npm-run-all clean:dist build:tsc",
        "build:tsc": "tsc --build ./tsconfig.json",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "npm-run-all clean:test build:tsc test:jasmine",
        "test:jasmine": "jasmine --require=source-map-support/register",
        "clean:dist": "rimraf ./dist",
        "clean:test": "rimraf ./spec/dist"
    },
    "keywords": [
        "discord",
        "bot"
    ],
    "homepage": "https://github.com/FelixRilling/lisa-bot",
    "author": {
        "name": "Felix Rilling",
        "email": "contact@rilling.dev",
        "url": "https://rilling.dev"
    },
    "repository": "https://github.com/FelixRilling/lisa-bot",
    "license": "MIT",
    "dependencies": {
        "source-map-support": "^0.5.19",
        "discord.js": "github:discordjs/discord.js#d827544fbd12e827fb4b6ff99d8894ecd79ede02",
        "discord.js-commando": "github:discordjs/Commando#bdbd84e2a978322f3af12c21e729eb7b7cd4a3dc",
        "fs-extra": "^9.0.0",
        "inversify": "^5.0.1",
        "lightdash": "^12.0.1",
        "lodash": "^4.17.15",
        "moment": "^2.25.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.5",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.0.1",
        "@types/jasmine": "^3.5.10",
        "@types/lodash": "^4.14.150",
        "@types/node": "^13.13.5",
        "@types/ws": "^7.2.4",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.31.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "jasmine": "^3.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.1",
        "ts-mockito": "^2.5.0",
        "typescript": "^3.8.3"
    }
}
