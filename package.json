    "license": "MIT",
    "dependencies": {
        "di-ngy": "8.0.0-4",
        "lightdash": "^9.1.0",
        "make-fetch-happen": "^4.0.1",
        "moment": "^2.22.2",
        "promise-queue": "^2.2.5"
    },
    "devDependencies": {
        "eslint": "^5.9.0",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.15.2"
    }
